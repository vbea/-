<?xml version="1.0" encoding="utf-8"?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<gradient
				android:startColor="#EF68B1F5" 
				android:endColor="#EF68B1F5"
				android:angle="270"/>
			<stroke android:width="0px" android:color="#cccccc"/>
			<corners android:radius="0dp"/>
		</shape>
	</item>
	<item>
		<shape>
			<gradient
				android:startColor="#EEEEEEEE" 
				android:endColor="#EEEEEEEE"
				android:angle="270"/>
			<stroke android:width="0px" android:color="#cccccc"/>
			<corners android:radius="0dp"/>
		</shape>
	</item>
</selector>
