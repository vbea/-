<?xml version="1.0" encoding="utf-8"?>
<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:id="@android:id/background">
		<shape>
			<corners
				android:radius="0dp"/>
			<gradient
				android:startColor="#00000000"
				android:centerColor="#00000000"
				android:endColor="#00000000"
				android:angle="180"/>
			<stroke android:width="1dp" android:color="#cccccc"/>
		</shape>
	</item>
	<item
		android:id="@android:id/secondaryProgress">
		<clip>
			<shape>
				<corners
					android:radius="0dp"/>
				<gradient
					android:startColor="#5FF165"
					android:centerColor="#FFF028"
					android:endColor="#FF88F7"
					android:angle="0"/>
			</shape>
		</clip>
	</item>
	<item
		android:id="@android:id/progress">
		<clip>
			<shape>
				<corners
					android:radius="0dp"/>
				<gradient
					android:startColor="#02DF82"
					android:centerColor="#FFD306"
					android:endColor="#FF0C10"
					android:angle="0"/>
			</shape>
		</clip>
	</item>
</layer-list>
